    <div class="scroll-container fadeInUpFast-animation">
        {% for link in photoCredits.linksDoc %}
        <div class="photoCredit scroll-page">
            <p><b>{{ link.date }}</b></p>
            <p>{{ link.publicist }}</p>
            <p>{{ link.title }}</p>
            <p>
              <b>
                <a href="{{ link.url }}" target="_blank" rel="noopener noreferrer">
                  <svg width="1.2rem" height="1.2rem" viewBox="0 0 24 24" style="cursor:pointer">
                    <g stroke-width="2.1" stroke="white" fill="none" stroke-linecap="round" stroke-linejoin="round">
                      <polyline points="17 13.5 17 19.5 5 19.5 5 7.5 11 7.5"></polyline>
                      <path d="M14,4.5 L20,4.5 L20,10.5 M20,4.5 L11,13.5"></path>
                    </g>
                  </svg>
                 </a>
               </b>
             </p>
        </div>
        {% endfor %}
    </div>
